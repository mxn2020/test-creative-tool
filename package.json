    "autoprefixer": "^10.4.20",
    "better-auth": "^1.2.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "mongodb": "^6.17.0",
    "next-themes": "^0.4.6",
    "prisma": "^6.12.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.0",
